band_members %>% semi_join(band_instruments)
band_members %>% anti_join(band_instruments)

## -----------------------------------------------------------------------------
band_members[band_members$name %in% band_instruments$name, , drop = FALSE]
band_members[!band_members$name %in% band_instruments$name, , drop = FALSE]

